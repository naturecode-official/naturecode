{
  "name": "naturecode",
  "version": "1.4.6",
  "description": "Cross-platform terminal AI assistant",
  "type": "module",
  "main": "src/cli/index.js",
  "bin": {
    "naturecode": "src/cli/index.js"
  },
  "scripts": {
    "dev": "node src/cli/index.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,ts,json}\"",
    "build": "pkg . --targets node18-linux-x64,node18-macos-x64,node18-win-x64 --out-path dist/"
  },
  "keywords": [
    "ai",
    "terminal",
    "cli",
    "deepseek",
    "chat"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "pkg": "^5.8.1",
    "prettier": "^3.1.1"
  }
}
